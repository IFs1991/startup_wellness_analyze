# アプリケーション設定
APP_NAME=startup-wellness
PROJECT_NAME=Startup Wellness Data Analysis System
VERSION=0.1.0
DEBUG=True
ENVIRONMENT=development

# サーバー設定
HOST=0.0.0.0
PORT=8000
WORKERS=4

# PostgreSQL基本設定
DB_ENGINE=postgresql
DB_NAME=startup_wellness_analyze
DB_USER=postgres
DB_PASSWORD=your_secure_password
DB_HOST=localhost
DB_PORT=5432
DB_SCHEMA=public

# PostgreSQLプール設定
POSTGRES_POOL_SIZE=5
POSTGRES_MAX_OVERFLOW=10
POSTGRES_POOL_TIMEOUT=30
POSTGRES_POOL_RECYCLE=1800

# PostgreSQL接続設定
DB_CONNECT_TIMEOUT=60
DB_COMMAND_TIMEOUT=30
DB_SSL_MODE=disable
DATABASE_URL=postgresql://postgres:your_secure_password@localhost:5432/startup_wellness_analyze

# データベース接続リトライ設定
DB_RETRY_LIMIT=5
DB_RETRY_INTERVAL=10

# PostgreSQL Docker設定
POSTGRES_DB=startup_wellness_analyze
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password

# Firebase設定
FIREBASE_PROJECT_ID=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=./credentials/your-project-id-firebase-adminsdk-xxxx-xxxxxxxx.json
FIREBASE_DATABASE_URL=https://your-project-id.firebaseio.com
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# API設定
SECRET_KEY=your-development-secret-key
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
ALGORITHM=HS256

# CORS設定
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["GET", "POST", "PUT", "DELETE", "OPTIONS"]
CORS_ALLOW_HEADERS=["*"]

# ログ設定
LOG_LEVEL=INFO
LOG_FORMAT=json
ENABLE_CLOUD_LOGGING=false
LOG_FILE_PATH=logs/app.log
MAX_LOG_FILE_SIZE=10MB
LOG_ROTATION_COUNT=5

# 分析設定
ANALYSIS_DATA_CACHE_TTL=3600
ANALYSIS_RESULT_STORAGE_DAYS=30
ANALYSIS_MAX_CONCURRENT_JOBS=5

# ベイズ分析設定
MCMC_DEFAULT_SAMPLES=2000
MCMC_DEFAULT_TUNE=1000
MCMC_DEFAULT_CHAINS=4
MCMC_RANDOM_SEED=42

# BigQuery設定
BIGQUERY_PROJECT_ID=your-project-id
BIGQUERY_DATASET_ID=analysis_results
BIGQUERY_LOCATION=US

# キャッシュ設定
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# フロントエンド設定
VITE_API_URL=http://localhost:8000
NODE_ENV=development

# PostgreSQL本番データベース設定
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=startup_wellness_analyze
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password

# PostgreSQLテストデータベース設定
POSTGRES_TEST_HOST=localhost
POSTGRES_TEST_PORT=5432
POSTGRES_TEST_DB=test_startup_wellness
POSTGRES_TEST_USER=your-test-db-user
POSTGRES_TEST_PASSWORD=your-test-db-password

# PostgreSQL接続プール設定
POSTGRES_POOL_SIZE=5
POSTGRES_MAX_OVERFLOW=10
POSTGRES_POOL_TIMEOUT=30
POSTGRES_POOL_RECYCLE=1800

# その他の設定
POSTGRES_ECHO=False
POSTGRES_TIMEZONE=UTC

# Firebase/GCP設定
GCP_PROJECT_ID=your-gcp-project-id